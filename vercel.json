{
  "version": 2,
  "builds": [
    {
      "src": "vercel-server.js",
      "use": "@vercel/node"
    },
    {
      "src": "dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "vercel-server.js"
    },
    {
      "src": "/(.*)",
      "dest": "vercel-server.js"
    }
  ]
} 